extends ./layout.pug
block content
  // navbar
  nav.navbar.navbar-expand-md.navbar-light.justify-content-between.fixed-top
    a.navbar-brand(href='/')
       img.logo.my-2(src='../img/logo_edited1.png' alt='logo')
    button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarsExampleDefault' aria-controls='navbarsExampleDefault' aria-expanded='false' aria-label='Toggle navigation')
      span.navbar-toggler-icon
    #navbarsExampleDefault.collapse.navbar-collapse.justify-content-end
      ul.navbar-nav
        li.nav-item
          a.nav-link(href='/')
            | Home
            span.sr-only (current)
        li.nav-item
          a.nav-link(href='/about') About
        li.nav-item
          a.nav-link(href='/menu') Menu
        li.nav-item
          a.nav-link(href='/services') Services
        li.nav-item
          a.nav-link(href='/contact') Contact
        li.nav-item.active
          a.nav-link(href='/register') Register
        li.nav-item
          a.nav-link(href='/login') Login
      .ml-3.top-search
        // util search
        form.form-inline(method='get' action='')
          input(type='hidden' name='_token' value='')
          input#query.form-control.searchbox(type='search' name='query' placeholder='Search')
          span.input-group-btn
            button.btn.btn-outline-info.my-2.my-sm-0.text-white.bu(type='submit') Search
      .menu-top-wrapper
        .ml-3.acct_user.d-flex
          // util profile
          a(href='/profile')
            .circle1
              .user_image
                i.fas.fa-user-alt.text-white
        .ml-2.acct_srch.d-flex
          // shopping cart
          .ml-2.circle
            a(href='/cart')
              i.fas.fa-shopping-cart.text-white
  // /navbar
  header.masthead
    .container.d-flex.h-100.align-items-center.justify-content-center.register
      .row.justify-content-center
        h1.text-black.mx-auto.mt-0.mb-3.text-center.custom_lineheight Register
        .col-md-12
          .card.re_form
            .card-header Register
            .card-body
              if messages
                local.messages
              form(.register method='POST' action='/register')
                .form-group.row
                  label.col-md-4.col-form-label.text-md-right(for='name') Name
                  .col-md-6
                    input#name.form-control(type='text' name='name' value='' required='' autocomplete='name' autofocus='')
                .form-group.row
                  label.col-md-4.col-form-label.text-md-right(for='email') E-Mail Address
                  .col-md-6
                    input#email.form-control(type='email' name='email' value=''  required='' placeholder='cuisine@example.com', autofocus='', pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,}$" autocomplete='email')
                .form-group.row
                  label.col-md-4.col-form-label.text-md-right(for='password') Password
                  .col-md-6
                    input#password.form-control(type='password' name='password' required=''  placeholder='minimum 7 characters', autocomplete='new-password')
                .form-group.row
                  label.col-md-4.col-form-label.text-md-right(for='password-confirm') Confirm Password
                  .col-md-6
                    input#password-confirm.form-control(type='password' name='password2' required='' autocomplete='new-password')
                .form-group.row.mb-0
                  .col-md-6.offset-md-4
                    input(type="hidden" name="" value="")
                    button.btn.btn-info(type='submit')
                      | Register
  // footer area
  footer.text-center.text-white
    // Grid container
    .container.pt-4
      // Section: Social media
      section.mb-4
        // Instagram
        a.btn.btn-primary.btn-floating.m-1(style='background-color: #ac2bac;' href='https://www.instagram.com/rahwa_netsereab/' role='button')
          i.fab.fa-instagram
        // /Instagram
        // Linkedin
        a.btn.btn-primary.btn-floating.m-1(style='background-color: #0082ca;' href='https://www.linkedin.com/in/rahwa-mehreteab/' role='button')
          i.fab.fa-linkedin-in
        // /Linkedin
        // Github
        a.btn.btn-primary.btn-floating.m-1(style='background-color: #333333;' href='https://github.com/Rahwa-n' role='button')
          i.fab.fa-github
        // /Github
        br
        br
        a.mail.text-white(href='mailto:rahwa@example.com?subject=About the traditional food')
          i.fa.fa-envelope(style='font-size:24px;')
          | Click here to send us an email
      // Section: Social media
    // Grid container
    // Copyright
    .text-center.text-white.p-3
      | Copyright &copy; Eritrean Traditional Food 2021
    // Copyright
