  // jQuery first, then Popper.js, then Bootstrap JS
  script(src="https://www.gstatic.com/firebasejs/8.6.7/firebase-app.js")
  script(src="https://www.gstatic.com/firebasejs/7.14.1/firebase-auth.js")
